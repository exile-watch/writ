{
  "name": "@exile-watch/writ-icons",
  "version": "0.2.3",
  "description": "Shared design system of React components for exile.watch fronted projects",
  "homepage": "https://github.com/exile-watch/writ/pkgs/npm/writ-icons",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exile-watch/writ.git",
    "directory": "packages/writ-icons"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "_clean": "del-cli dist",
    "_build": "rollup -c",
    "build": "run-s _clean _build",
    "typecheck": "tsc --noEmit",
    "svgr": "npx @svgr/cli --svgo-config .svgrrc.js --out-dir src/icons -- src/svg"
  },
  "devDependencies": {
    "@exile-watch/rollup-config": "^0.1.1",
    "@svgr/cli": "^8.1.0",
    "@types/react": "^18.2.55",
    "del-cli": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
